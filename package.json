{
  "name": "wetubecrud",
  "version": "1.0.0",
  "engines": {
    "node": "12.18.2"
  },
  "description": "Cloning Youtube with ValilnaJS and NodeJS",
  "main": "index.js",
  "dependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/polyfill": "^7.12.1",
